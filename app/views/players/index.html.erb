<div class="container-fluid">

  <div class="row">
    <div>
      <label for="search">Search:</label>
    </div>
    <div>
      <input placeholder="Type Player Name" type="text" class="validate" id="search" name="search">
    </div>
  </div>
  <div class="row">
    <table id="player-data">
      <tbody>
        <% @printable_player.each do |player| %>
          <tr data-id="<%= player[0] %>">
            <% player.each do |data_point| %>
              <td>
                <%= data_point %>
              </td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
